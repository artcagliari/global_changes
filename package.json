    "build:prod": "npm run build && cd server && npm run build"
  },
  "dependencies": {
    "@vercel/blob": "^2.0.0",
    "@vercel/node": "^3.2.29",
    "busboy": "^1.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
